indeks_list={1:"Tallinn",2:"Narva",3:"Kohtla Jarve"}

while type(index) !=5 or index.isdigit()!=True:
    try:
        indeks=int(input("Index:"))
        
    except:
        ValueError

city=index_list.get(int(list(index)[0]))
